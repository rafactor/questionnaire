<!-- Two Line List with secondary info and action -->
<style>
    .demo-list-two {
        width: 300px;
    }
</style>
<h5>List of all questionnaires</h5>
<ul class="demo-list-two mdl-list">
    <li class="mdl-list__item mdl-list__item--two-line">
        <span class="mdl-list__item-primary-content">
            <i class="material-icons mdl-list__item-avatar">check_circle</i>
            <span>{{ name }}</span>
            <span class="mdl-list__item-sub-title">{{ nQuestions }}</span>
        </span>
        <span class="mdl-list__item-secondary-content">
            <span class="mdl-list__item-secondary-info">Status</span>
            <a class="mdl-list__item-secondary-action" href="#"><i class="material-icons">radio_button_checked</i></a>
        </span>
    </li>
</ul>

